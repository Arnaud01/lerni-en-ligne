	$maparea.on("mousewheel", function(e) {
		if (e.deltaY > 0)
			$maparea.trigger("zoom", $maparea.data("zoomLevel") + 1);
		else
			$maparea.trigger("zoom", $maparea.data("zoomLevel") - 1);
			
		return false;
	});


});
